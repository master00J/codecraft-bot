# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_here

# Discord Stats (Optional - for displaying bot stats in support server as voice channels)
DISCORD_SUPPORT_SERVER_ID=1435653730799190058
DISCORD_STATS_CATEGORY_ID=your_stats_category_id_here

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_ANON_KEY=your_anon_key_here

# Webapp API
WEBAPP_API_URL=https://codecraft-solutions.com
WEBAPP_API_KEY=your_webapp_api_key_here

# AI Configuration (Optional)
GOOGLE_AI_API_KEY=your_google_ai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# AI Model Configuration (Optional)
GEMINI_MODEL=gemini-1.5-pro
CLAUDE_MODEL=claude-3-5-haiku-latest
DEEPSEEK_MODEL=deepseek-chat

# AI Cost Tracking (Optional - prices per 1K tokens)
# Gemini Pricing
AI_COST_GEMINI_INPUT_PER_1K=0.0
AI_COST_GEMINI_OUTPUT_PER_1K=0.0

# Claude Pricing
AI_COST_CLAUDE_INPUT_PER_1K=0.0
AI_COST_CLAUDE_OUTPUT_PER_1K=0.0

# DeepSeek Pricing (V3.2)
# Input: $0.28 per 1M tokens (cache miss) = $0.00028 per 1K tokens
# Input: $0.028 per 1M tokens (cache hit) = $0.000028 per 1K tokens
# Output: $0.42 per 1M tokens = $0.00042 per 1K tokens
# Recommended: Use weighted average based on expected cache hit rate
# Example (50% cache hit): (0.028 * 0.5 + 0.28 * 0.5) / 1000 = 0.000154
AI_COST_DEEPSEEK_INPUT_PER_1K=0.000154
AI_COST_DEEPSEEK_OUTPUT_PER_1K=0.00042

# Fallback cost rates (if provider-specific rates not set)
AI_COST_INPUT_PER_1K=0.0
AI_COST_OUTPUT_PER_1K=0.0

# Top.gg (Optional)
TOPGG_TOKEN=your_topgg_token_here
TOPGG_WEBHOOK_AUTH=your_topgg_webhook_auth_here

# DiscordBotList.com (Optional)
DISCORDBOTLIST_TOKEN=your_discordbotlist_token_here

# Twitch (Optional)
TWITCH_CLIENT_ID=your_twitch_client_id_here
TWITCH_CLIENT_SECRET=your_twitch_client_secret_here

# Environment
NODE_ENV=production
